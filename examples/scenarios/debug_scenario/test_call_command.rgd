label test_call_command:
	# Call/Returnコマンドのテストシナリオ

	"Call/Returnのテストを開始します"

	call function_a

	"関数Aから戻ってきました"

	call function_b

	"関数Bから戻ってきました"

	"Call/Returnテスト完了"

label function_a:
	"関数A: 処理開始"
	"関数A: 何らかの処理を実行中..."
	"関数A: 処理完了"
	return

label function_b:
	"関数B: 処理開始" 
	call function_c
	"関数B: 関数Cから戻ってきました"
	"関数B: 処理完了"
	return

label function_c:
	"関数C: ネストした処理を実行"
	"関数C: 深い処理..."
	return
